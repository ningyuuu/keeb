{
  "name": "tailwindcss-parcel-starter",
  "version": "1.0.0",
  "description": "A simple tailwind starter project with parcel",
  "scripts": {
    "dev": "yarn parcel src/index.html",
    "prod": "yarn parcel build src/index.html",
    "predeploy": "rm -rf dist && parcel build src/index.html --public-url /keeb",
    "deploy": "gh-pages -d dist"
  },
  "repository": "https://github.com/SantiMA10/tailwindcss-parcel-starter.git",
  "author": "Santi <santiagomartinagra@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@parcel/optimizer-cssnano": "2.0.0-nightly.605",
    "@parcel/optimizer-htmlnano": "2.0.0-nightly.605",
    "@parcel/packager-css": "2.0.0-nightly.605",
    "@parcel/packager-html": "2.0.0-nightly.605",
    "@parcel/transformer-css": "2.0.0-nightly.605",
    "@parcel/transformer-html": "2.0.0-nightly.605",
    "@parcel/transformer-postcss": "2.0.0-nightly.605",
    "@parcel/transformer-posthtml": "2.0.0-nightly.605",
    "autoprefixer": "^10.2.4",
    "parcel": "^2.0.0-nightly.540",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "gh-pages": "^3.1.0",
    "posthtml-include": "^1.6.2",
    "posthtml-inline-svg": "^0.2.0",
    "tailwind-hamburgers": "^1.0.2"
  }
}
